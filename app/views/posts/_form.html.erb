<div class="container">
  <%= form_for @post do |f| %>
    <p>
      <%= f.label :title %> <br>
      <%= f.text_field :title, size:"50x5" %>
    </p>
    <p>
      <%= f.label :body %>  <br>
      <%= f.text_area :body, size:"100x30" %>
    </p>
    <p>
      <%= f.submit %>
    </p>
  <% end  %>
</div>